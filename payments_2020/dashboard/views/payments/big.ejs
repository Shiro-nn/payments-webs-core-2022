            }
            .btn, a, a * {
                cursor: url(/cursors/cyan/pointer.cur), pointer !important;
            }
            p, h4 {
                cursor: url(/cursors/cyan/select.cur), pointer !important;
            }
            ::-moz-selection { 
            color: cyan;
            background: #00000020;
            border-radius: 15px;
            }
            ::selection {
            color: cyan;
            background: #00000020;
            }
