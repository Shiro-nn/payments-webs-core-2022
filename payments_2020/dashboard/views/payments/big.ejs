        <script>
        var isFramed = false;
        try {
          isFramed = window != window.top || document != top.document || self.location != top.location;
        } catch (e) {
          isFramed = true;
        }
        if (isFramed) {
            window.location.href = `/payments/<%= sdata.mid %>/widget?id=<%= id %>&sum=<%= sum %>`;
            window.top.location.href = `/payments/<%= sdata.mid %>?id=<%= id %>&sum=<%= sum %>`;
